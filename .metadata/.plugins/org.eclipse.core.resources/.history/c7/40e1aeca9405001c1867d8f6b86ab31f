package p1;

/*
 * 
 * */
public class Ex5 {
	public static void main(String[] args) {
		static Ex4 ex; // null reference variable.
		int x = 10;

		public static void main(String[] args) {
			try{
				System.out.println(ex.x);
				}catch(NullPointerException e) {// only handles null pointer 
					// it can't handle any other exception.
					e.printStackTrace();
			}
		}
	}
}
