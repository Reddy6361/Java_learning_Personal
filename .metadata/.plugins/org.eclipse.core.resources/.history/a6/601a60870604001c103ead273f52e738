package p1;
// Exception example 2
public class Ex8 {
	public static void main(String[] args) {
		try {
			int x = 20;
			int y = 0;
			int z = x / y; // after exception no code will run
			System.out.println(z);// will not run
			System.out.println("hello");
		} catch (Exception e) {
			System.out.println(e);
		}
	}
}
