package p1;

/*
 * Example 3 Arrays.
 * Note: a[1] => starting address + memory size (index number)
 * */
public class Ex9 {
	public static void main(String[] args) {
		int[] arr = new int[4];
		arr[0] = 10;
		arr[1] = 20;
		arr[2] = 30;
		System.out.println(arr[0]); // 10
		System.out.println(arr[1]); // 20
		System.out.println(arr[2]); // 30
		System.out.println(arr[3]);
	}
}
