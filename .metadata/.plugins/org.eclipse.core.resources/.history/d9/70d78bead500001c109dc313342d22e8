package p1;
// super key is not auto kept when there are only constructors with args
//in parent class, where as super keyword will be placed automatically
//when in parent class there is construictoir witgh no args.
public class Ex11 {
	Ex11(int i){
		System.out.println("parent class constructor");
		System.out.println(i);
	}
	Ex11(){
		System.out.println("from no args constructor.");
	}
}
